    <select id="getQyhUserVoByUserId" resultType="com.ziwow.scrmapp.common.bean.vo.QyhUserVo"
            parameterType="java.lang.String">
        SELECT
            round(AVG(a.attitude), 2) AS attitude,
            round(AVG(a.profession), 2) AS profession,
            round(AVG(a.integrity), 2) AS integrity,
            u. NAME,
            u.mobile,
            u.position,
            u.avatar AS imageUrl
        FROM
            t_qyh_user u
        LEFT JOIN t_qyh_user_appraisal a ON (
            u.userId = a.qyhUserId
            AND a.attitude > 0
            AND a.integrity > 0
            AND a.profession > 0
        )
        WHERE
        u.userId = #{userId,jdbcType=VARCHAR}
    </select>

    <select id="getActiveUser" resultMap="BaseResultMap">
        SELECT
        userId,mobile
        FROM
            t_qyh_user
        WHERE
            STATUS = 1
        AND NAME NOT LIKE '离职%'
        AND departments NOT IN (1, 52)
